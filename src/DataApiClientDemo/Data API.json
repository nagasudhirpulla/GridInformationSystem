{
  "openapi": "3.0.1",
  "info": {
    "title": "DataApi | Data API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://localhost:7073/"
    }
  ],
  "paths": {
    "/Auth/Login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "string"
              }
            },
            "text/json": {
              "schema": {
                "type": "string"
              }
            },
            "application/*+json": {
              "schema": {
                "type": "string"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/JwtResponseDto"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JwtResponseDto"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/JwtResponseDto"
                }
              }
            }
          }
        }
      }
    },
    "/MeasData/GetData": {
      "post": {
        "tags": [
          "MeasData"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GetMeasurementDataQuery"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/GetMeasurementDataQuery"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/GetMeasurementDataQuery"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MeasurementDataDto"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MeasurementDataDto"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MeasurementDataDto"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/MeasData/InsertData": {
      "post": {
        "tags": [
          "MeasData"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InsertDataCommand"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/InsertDataCommand"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/InsertDataCommand"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GetMeasurementDataQuery": {
        "type": "object",
        "properties": {
          "measId": {
            "type": "integer",
            "format": "int32"
          },
          "fromTime": {
            "type": "string",
            "format": "date-time"
          },
          "toTime": {
            "type": "string",
            "format": "date-time"
          },
          "jsonPayload": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "InsertDataCommand": {
        "required": [
          "samples"
        ],
        "type": "object",
        "properties": {
          "samples": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/InsertDataRecord"
            }
          }
        }
      },
      "InsertDataRecord": {
        "type": "object",
        "properties": {
          "measId": {
            "type": "integer",
            "format": "int32"
          },
          "timestamp": {
            "type": "integer",
            "format": "int32"
          },
          "value": {
            "type": "number",
            "format": "float"
          }
        }
      },
      "JwtResponseDto": {
        "required": [
          "token",
          "expiration"
        ],
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "expiration": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "MeasurementDataDto": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int32"
          },
          "value": {
            "type": "number",
            "format": "float"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "Auth"
    },
    {
      "name": "MeasData"
    }
  ]
}